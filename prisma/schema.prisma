
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model book{
  id String @id @default(uuid())
  
  name String
  synopsis String
  value Float
  genders String

  createdAt DateTime @default(now())
  upDatedAt DateTime @updatedAt

  //Usar TimeStamp (tipo) para fusos diferentes
}